{
  "rewrites": [
    {
      "source": "/public/:path*",
      "destination": "/api/grafana-proxy?path=/public/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/grafana-proxy?path=/api/:path*"
    },
    {
      "source": "/d/:path*",
      "destination": "/api/grafana-proxy?path=/d/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
