{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/public/build/:path*",
      "destination": "/api/grafana-proxy?path=/public/build/:path*"
    },

    {
      "source": "/api/datasources/:path*",
      "destination": "/api/grafana-proxy?path=/api/datasources/:path*"
    },

    {
      "source": "/d/:path*",
      "destination": "/api/grafana-proxy?path=/d/:path*"
    },

    {
      "source": "/:path((?!api|public).*)*",
      "destination": "/"
    }
  ]
}
