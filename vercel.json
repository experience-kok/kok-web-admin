{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/public/build/:path*",
      "destination": "/api/grafana-proxy?path=/public/build/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/grafana-proxy?path=/api/:path*"
    },
    {
      "source": "/d/:path*",
      "destination": "/api/grafana-proxy?path=/d/:path*"
    },
    {
      "source": "/plugins/:path*",
      "destination": "/api/grafana-proxy?path=/plugins/:path*"
    },
    {
      "source": "/:path((?!api|public|d|plugins).)*",
      "destination": "/"
    }
  ]
}
