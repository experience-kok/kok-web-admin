{
  "rewrites": [
    {
      "source": "/public/:path*",
      "destination": "/api/grafana-proxy?path=/public/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/grafana-proxy?path=/api/:path*"
    },
    {
      "source": "/d/:path*",
      "destination": "/api/grafana-proxy?path=/d/:path*"
    },
    {
      "source": "/api/live/:path*",
      "destination": "https://grafana.chkok.kr/api/live/:path*"
    },
    {
      "source": "/api/frontend-metrics",
      "destination": "/api/grafana-proxy?path=/api/frontend-metrics"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
